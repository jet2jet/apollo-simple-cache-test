{
  "name": "apollo-simple-cache-test",
  "version": "0.1.0",
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "engineStrict": true,
  "description": "Test repository for apollo-simple-cache",
  "author": "jet",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "postinstall": "node scripts/postinstall.cjs",
    "benchmark": "tsx --tsconfig tsconfig.json src/main/benchmark.mts",
    "benchmark:with-gc": "tsx --tsconfig tsconfig.json --expose-gc src/main/benchmark.mts --with-gc",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint",
    "lint:eslint:fix": "eslint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "typecheck": "tsc --project tsconfig.json --noEmit && echo \"Done.\""
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@urql/exchange-graphcache": "^7.2.4",
    "apollo-simple-cache": "^0.3.0",
    "msw": "^2.10.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "urql": "^4.2.2"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "heapdump": "^0.3.15",
    "jsdom": "^26.1.0",
    "neostandard": "^0.12.2",
    "prettier": "^3.6.2",
    "tinybench": "^4.0.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0"
  }
}
